{
	"npmScope": "nxrs",
	"affected": {
		"defaultBase": "main"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"],
				"parallel": 1
			}
		}
	},
	"workspaceLayout": {
		"appsDir": "e2e",
		"libsDir": "packages"
	},
	"defaultProject": "cargo",
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["production", "^production"]
		}
	},
	"namedInputs": {
		"default": ["{projectRoot}/**/*", "sharedGlobals"],
		"sharedGlobals": [],
		"production": ["default"]
	}
}
